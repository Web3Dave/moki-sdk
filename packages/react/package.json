{
  "name": "@moki-chat/react",
  "version": "0.1.0",
  "description": "React SDK for Moki Chat - Web3 messaging and chat protocol",
  "type": "module",
  "module": "./_esm/index.js",
  "types": "./_types/index.d.ts",
  "exports": {
    ".": {
      "types": "./_types/index.d.ts",
      "import": "./_esm/index.js"
    },
    "./hooks": {
      "types": "./_types/hooks/index.d.ts",
      "import": "./_esm/hooks/index.js"
    },
    "./context": {
      "types": "./_types/context/index.d.ts",
      "import": "./_esm/context/index.js"
    }
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "sideEffects": false,
  "files": [
    "_esm",
    "_types"
  ],
  "scripts": {
    "build": "tsc",
    "build:bundle": "tsup index.ts --format esm --dts --minify --external viem --external @noble/curves --external scale-ts",
    "dev": "tsc --watch",
    "test": "vitest --config ../../vitest.config.ts",
    "test:cov": "vitest --config ../../vitest.config.ts --coverage",
    "test:ui": "vitest --config ../../vitest.config.ts --ui"
  },
  "packageManager": "pnpm@10.20.0",
  "keywords": [
    "moki",
    "moki-chat",
    "sdk",
    "react",
    "web3",
    "chat",
    "messaging",
    "ethereum",
    "blockchain"
  ],
  "author": {
    "name": "Web3Daveloper",
    "url": "https://github.com/Web3Dave"
  },
  "contributors": [
    {
      "name": "Web3Daveloper",
      "url": "https://github.com/Web3Dave"
    }
  ],
  "license": "MIT",
  "homepage": "https://github.com/Web3Dave/moki-sdk#readme",
  "bugs": {
    "url": "https://github.com/Web3Dave/moki-sdk/issues"
  },
  "peerDependencies": {
    "@moki-chat/core": ">=0.1.3",
    "react": ">=17.0.0"
  },
  "dependencies": {
    "@noble/curves": "~1.9.0",
    "scale-ts": "^1.6.1",
    "viem": "^2.38.6"
  },
  "devDependencies": {
    "@moki-chat/core": "workspace:*",
    "@types/react": "19.2.2",
    "@vitest/coverage-v8": "^4.0.6",
    "react": "^19.2.0",
    "tsup": "^8.5.0",
    "vitest": "^4.0.6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Web3Dave/moki-sdk"
  }
}