{
  "name": "@moki-chat/core",
  "version": "0.1.2",
  "description": "Core SDK for Moki Chat - Web3 messaging and chat protocol",
  "type": "module",
  "module": "./_esm/index.js",
  "types": "./_types/index.d.ts",
  "exports": {
    ".": {
      "types": "./_types/index.d.ts",
      "import": "./_esm/index.js"
    },
    "./accounts": {
      "types": "./_types/accounts/index.d.ts",
      "import": "./_esm/accounts/index.js"
    },
    "./data": {
      "types": "./_types/data/index.d.ts",
      "import": "./_esm/data/index.js"
    },
    "./enum": {
      "types": "./_types/enum/index.d.ts",
      "import": "./_esm/enum/index.js"
    },
    "./messaging": {
      "types": "./_types/messaging/index.d.ts",
      "import": "./_esm/messaging/index.js"
    },
    "./provider": {
      "types": "./_types/provider/index.d.ts",
      "import": "./_esm/provider/index.js"
    },
    "./persistor": {
      "types": "./_types/persistor/index.d.ts",
      "import": "./_esm/persistor/index.js"
    },
    "./utils": {
      "types": "./_types/utils/index.d.ts",
      "import": "./_esm/utils/index.js"
    },
    "./types": {
      "types": "./_types/types/index.d.ts",
      "import": "./_esm/types/index.js"
    }
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "sideEffects": false,
  "files": [
    "_esm",
    "_types"
  ],
  "scripts": {
    "build": "tsc",
    "build:bundle": "tsup index.ts --format esm --dts --minify --external viem --external @noble/curves --external scale-ts",
    "dev": "tsc --watch",
    "test": "vitest --config ../vitest.config.ts",
    "test:cov": "vitest --config ../vitest.config.ts --coverage",
    "test:ui": "vitest --config ../vitest.config.ts --ui"
  },
  "packageManager": "pnpm@10.20.0",
  "keywords": [
    "moki",
    "moki-chat",
    "sdk",
    "web3",
    "chat",
    "messaging",
    "ethereum",
    "blockchain"
  ],
  "author": {
    "name": "Web3Daveloper",
    "url": "https://github.com/Web3Dave"
  },
  "contributors": [
    {
      "name": "Web3Daveloper",
      "url": "https://github.com/Web3Dave"
    }
  ],
  "license": "MIT",
  "homepage": "https://github.com/Web3Dave/moki-sdk#readme",
  "bugs": {
    "url": "https://github.com/Web3Dave/moki-sdk/issues"
  },
  "dependencies": {
    "@noble/curves": "~1.9.0",
    "scale-ts": "^1.6.1",
    "viem": "^2.38.6"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^4.0.6",
    "tsup": "^8.5.0",
    "vitest": "^4.0.6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Web3Dave/moki-sdk"
  }
}